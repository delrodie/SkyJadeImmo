<form method="post" action="{{ path('app_backend_maintenance_delete', {'id': maintenance.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer cette maintenance?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ maintenance.id) }}">
    <button class="btn btn-danger" >Supprimer</button>
</form>
