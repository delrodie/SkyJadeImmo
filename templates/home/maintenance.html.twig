<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{% block title %}SkyJade {% endblock %}</title>
    <link rel="icon" href="{{ absolute_url(asset('build/images/glyph.png')) }}">

    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {% block stylesheets %}
        {{ encore_entry_link_tags('intro') }}
    {% endblock %}
</head>
<body class="accueil d-flex h-100 text-white bg-dark">
<div class="container">
    <div class="row justify-content-center">
        <div class="col-4 text-center"  data-aos="fade-down" data-aos-easing="linear" data-aos-duration="2500">
            <img src="{{ absolute_url(asset('build/images/glyph.png')) }}" alt="Glyph Skyjade" srcset="" class="img-fluid">
        </div>
    </div>
    <div class="row justify-content-center text-center palette">
        <div class="col-md-4 immobilier">
            <a href="#" data-aos="fade-down-right" data-aos-easing="linear" data-aos-duration="2500"><h3>Immobilier</h3></a>
        </div>
        <div class="col-md-4 automobile">
            <a href="#" data-aos="fade-up-left" data-aos-easing="linear" data-aos-duration="2500"><h3>Automobile</h3></a>
        </div>
    </div>
    <div class="row justify-content-center text-center">
        <div class="col maintenance">
            <h5>Site en maintenance...</h5>
        </div>
    </div>
</div>


{% block javascripts %}
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
    {{ encore_entry_script_tags('app') }}
    <script>
        AOS.init();
    </script>
{% endblock %}
</body>
</html>
